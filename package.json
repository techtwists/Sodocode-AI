{
  "name": "sodocode-ai",
  "displayName": "Sodocode AI",
  "description": "Generate code from OpenAI for Pseudocode .",
  "version": "0.0.4",
  "publisher": "PurebitOrg",
  "repository": {
    "type": "git",
    "url": "https://github.com/techtwists/Sodocode-AI.git"
  },
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "Title": "Sodocode AI",
      "properties": {
        "sodocode-ai.apiKey": {
          "type": "string",
          "default": "API Key!"
        },
        "sodocode-ai.temperature": {
          "type": "number",
          "default": 0
        },
        "sodocode-ai.maxToken": {
          "type": "number",
          "default": 1024
        }
      }
    },
    "commands": [
      {
        "command": "extension.generateSodocodeai",
        "title": "Generate with Sodocode AI",
        "category": "Generate"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.generateSodocodeai",
          "group": "2_generate",
          "when": "editorTextFocus"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "esbuild": "^0.17.11",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "dotenv": "^16.0.3"
  }
}
